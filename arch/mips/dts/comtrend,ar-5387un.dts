/dts-v1/;

#include "brcm,bcm6328.dtsi"

/ {
	model = "Comtrend AR-5387un";
	compatible = "comtrend,ar5387-un", "brcm,bcm6328";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&uart0 {
	u-boot,dm-pre-reloc;
	status = "okay";
};
